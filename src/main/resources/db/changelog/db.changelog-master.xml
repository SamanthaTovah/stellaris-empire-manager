<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                                       http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="create_appearance_table" author="Samantha Tovah">
        <createTable tableName="appearance">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="name" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

    <changeSet id="create_trait_table" author="Samantha Tovah">
        <createTable tableName="trait">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="name" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

    <changeSet id="create_planet_class_table" author="Samantha Tovah">
        <createTable tableName="planet_class">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="name" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

    <changeSet id="create_origin_table" author="Samantha Tovah">
        <createTable tableName="origin">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="name" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

    <changeSet id="create_species_table" author="Samantha Tovah">
        <createTable tableName="species">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="name" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="appearance" type="BIGINT">
                <constraints nullable="false" foreignKeyName="fk_species_appearance"
                             referencedTableName="appearance" referencedColumnNames="id"/>
            </column>
        </createTable>
        <createTable tableName="species_trait">
            <column name="species_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="trait_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
        </createTable>
        <addPrimaryKey tableName="species_trait" columnNames="species_id, trait_id" constraintName="pk_species_trait"/>
        <addForeignKeyConstraint constraintName="fk_species_trait_species"
                                 baseTableName="species_trait" baseColumnNames="species_id"
                                 referencedTableName="species" referencedColumnNames="id"/>
        <addForeignKeyConstraint constraintName="fk_species_trait_trait"
                                 baseTableName="species_trait" baseColumnNames="trait_id"
                                 referencedTableName="trait" referencedColumnNames="id"/>
    </changeSet>


    <changeSet id="create_homeworld_table" author="Samantha Tovah">
        <createTable tableName="homeworld">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="homeworld_name" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="star_name" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="planet_class" type="BIGINT">
                <constraints nullable="false" foreignKeyName="fk_homeworld_planet_class"
                             references="planet_class(id)"/>
            </column>
            <column name="origin" type="BIGINT">
                <constraints nullable="false" foreignKeyName="fk_homeworld_origin"
                             references="origin(id)"/>
            </column>
        </createTable>
    </changeSet>

    <changeSet id="create_empire_table" author="Samantha Tovah">
        <createTable tableName="empire">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="name" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="species" type="BIGINT">
                <constraints nullable="false" foreignKeyName="fk_empire_species"
                             references="species(id)"/>
            </column>
            <column name="homeworld" type="BIGINT">
                <constraints nullable="false" foreignKeyName="fk_empire_homeworld"
                             references="homeworld(id)"/>
            </column>
        </createTable>
    </changeSet>

